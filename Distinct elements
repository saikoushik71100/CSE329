#include<iostream>
using namespace std;
main(){
    int a[4][4],temp[1][4];
    cout<<"enter matrix";
    for(int i=0;i<4;i++){
        for(int j=0;j<4;j++){
            cin>>a[i][j];
        }
    }
     cout<<"Entered matrix is "<<endl;
      for(int i=0;i<4;i++){
        for(int j=0;j<4;j++){
            cout<<a[i][j]<<"\t";
        }
        cout<<"\n";
    }
    for(int i=0;i<1;i++){
    	for(int j=0;j<4;j++){
    		cin>>temp[i][j];
		}
	}
    for(int i=0;i<1;i++){
    	for(int j=0;j<4;j++){
    		cout<<temp[i][j]<<"\t";
		}
	}
	for(int i=1;i<3;i++){
		for(int j=0;j<3;j++){
			if(temp[i][j]==a[i][j])
			{
				cout<<temp[i][j];
			}
		}
	}
}
